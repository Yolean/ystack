#!/bin/sh
[ -z "$DEBUG" ] || set -x
set -e
YBIN="$(dirname $0)"

version=0.8.1

bin_name=dive \
  bin_version=v${version} \
  Darwin_url=https://github.com/wagoodman/dive/releases/download/v${version}/dive_${version}_darwin_amd64.tar.gz \
  Darwin_sha256=bc2db11f3cf2a1ba9bb8839ced6cb9a87048c04fc65858b34a8a19ea82f0cc19 \
  bin_tgz_path=dive \
  y-bin-dependency-download || exit $?

y-dive-v${version}-bin "$@" || exit $?
